{
  "expo": {
    "name": "Smart Build System",
    "slug": "smart-build-system",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sbs",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81ef2108-dde8-4dd8-b822-9a73a55aef17"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "checkOnLaunch": "ALWAYS",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/81ef2108-dde8-4dd8-b822-9a73a55aef17"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kz.smartremont.sbs",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to your camera to allow you to capture and upload photos of your works and documents.",
        "NSFaceIDUsageDescription": "This app uses Face ID and Touch ID to verify the user's identity and protect sensitive data",
        "LSApplicationQueriesSchemes": ["http", "https", "http", "https"]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "kz.smartremont.sbs",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "This app uses Face ID and Touch ID to verify the user's identity and protect sensitive data"
        }
      ],
      "expo-font",
      "expo-asset",
      "expo-media-library",
      "expo-video",
      "expo-notifications",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ]
  }
}
